{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "react": [ "./node_modules/@types/react" ]
    },
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "../liveblocks/src/app/Room.tsx", "components/mode-toggle.js", "app/parser/page.jsx", "app/preferences/page.jsx", "components/Sidebar.jsx", "components/Sidebar.jsx", "components/Sidebar.jsx", "components/Sidebar.jsx"],
  "exclude": ["node_modules"]
}
